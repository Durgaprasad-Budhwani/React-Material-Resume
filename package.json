    "start": "react-scripts start",
    "build": "react-scripts build",
    "docs": "react-doc-generator src -o DOCUMENTATION.md",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "test-ci": "jest -u --testResultsProcessor='jest-junit'",
    "eject": "react-scripts eject",
    "flow": "flow",
    "codecov": "codecov"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
